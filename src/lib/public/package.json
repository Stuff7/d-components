{
  "name": "d-components",
  "version": "1.1.1",
  "description": "Vue Component Library",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./*": {
      "types": "./*.d.ts",
      "import": "./*.js"
    },
    "./*.vue": {
      "types": "./components/*.vue.d.ts",
      "import": "./components/*.js"
    }
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  }
}
